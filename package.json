    "hotkey",
    "hotkeys",
    "shortcut",
    "shortcuts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dherault/piano-keys.git"
  },
  "bugs": {
    "url": "https://github.com/dherault/piano-keys/issues"
  },
  "homepage": "https://github.com/dherault/piano-keys#readme",
